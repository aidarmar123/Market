<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MarketMAUI.Pages.CameraPage"
             xmlns:cm ="clr-namespace:Camera.MAUI;assembly=Camera.MAUI"
             Title="CameraPage">
    <VerticalStackLayout>
        <cm:CameraView x:Name="cameraView"
                       CamerasLoaded="Camera_CamerasLoaded"
                       BarcodeDetected="Camera_BarcodeDetected"
                     WidthRequest="300"
                     HeightRequest="200"/>
    </VerticalStackLayout>
</ContentPage>